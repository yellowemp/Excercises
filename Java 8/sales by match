public static int sockMerchant(int n, List<Integer> ar) {
    List<Integer> newar = ar.stream().sorted().collect(Collectors.toList());
        int l = 0;
        int cnt = 0;
        for (int x = 0; x <n-1; x++){
            l = x+1;
            if (newar.get(x).intValue() == newar.get(l).intValue()){
                cnt++;
                x++;
            }
        }
        return cnt;

    }